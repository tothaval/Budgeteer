             xmlns:local="clr-namespace:BudgeteerWPF"
             xmlns:view="clr-namespace:BudgeteerWPF.Views"
             xmlns:vm="clr-namespace:BudgeteerWPF.ViewModels">
    <Application.Resources>
        
        <DataTemplate DataType="{x:Type vm:BudgetServiceViewModel}">
            <view:BudgetServiceView />
        </DataTemplate>
        
    </Application.Resources>
</Application>
